  "glossary_of_terms": [
    { "title": "Automatic", "content": "Also referred to as a Square Bottom, Carry Out or Self-Opening bag." },
    { "title": "Basis Weight", "content": "The finished weight of a paper based on a consistent industry standard." },
    { "title": "Dry Wax", "content": "A process in which the paper is waxed lightly on one side. The finished paper is also referred to as Dry Wax(ed)." },
    {"title": "FDA Approved","content": "Paper that meets FDA 21 CFR requirements for direct/indirect food contact" },
    {"title": "Gusset", "content": "The sides of a bag, sometimes referred to as bellows, which are measured when open from front to back." },
    {"title": "Openside", "content": "A Pinch Bottom bag open on one side as well as the top, providing easy access for foods to be eaten directly from the package." },
    {"title": "Pinch Bottom","content": "A bag style, available flat or gusseted, having a pinched bottom, as opposed to a formed square bottom." },
    {"title": "PMS Colors", "content": "The Pantone Matching System is an international printing, publishing and packaging color language providing an accurate method for selecting and matching color." },
    {"title": "Waxed","content": "A process in which wax is applied to paper." }
  ]
